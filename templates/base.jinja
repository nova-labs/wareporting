    <!-- jQuery -->
    <script src="https://code.jquery.com/jquery-3.7.0.min.js" data-hash="sha256-2Pmvv0kuTBOenSvLm6bvfBSSHrUJ+3A7x6P5Ebd07/g="></script>
    {% block javascript %}{% endblock %}
